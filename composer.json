{
    "name": "wandxx/transaction",
    "description": "transaction library",
    "type": "laravel-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "wandy purnomo",
            "email": "wandypurnomo92@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Wandxx\\Transaction\\": "src/"
        },
        "files": [
            "src/Helpers/transaction_helper.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Wandxx\\Transaction\\Providers\\TransactionServiceProvider",
                "Wandxx\\Transaction\\Services\\Cart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Wandxx\\Transaction\\Services\\Cart\\CartServiceFacade",
                "TransactionItem": "Wandxx\\Transaction\\Services\\Cart\\Objects\\TransactionItemFacade"
            }
        }
    },
    "require": {
        "wandxx/support": "^1.0",
        "envant/fireable": "^1.0"
    }
}
